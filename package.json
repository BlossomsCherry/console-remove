{
  "name": "console-remove",
  "displayName": "Console Remove",
  "description": "快速删除项目中的控制台打印",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./src/index.js",
  "contributes": {
    "commands": [
      {
        "command": "console-remove.progress",
        "title": "显示进度条"
      },
      {
        "command": "console-remove.removeConsoleLogs",
        "title": "删除控制台打印"
      },
      {
        "command": "console-remove.removeAllConsoleLogs",
        "title": "remove-all-console"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "console-remove.removeConsoleLogs",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BlossomsCherry/console-remove.git"
  },
  "license": "MIT"
}
